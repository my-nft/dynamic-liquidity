    function _sqrt(uint _x) internal pure returns(uint y) {
        uint z = (_x + 1) / 2;
        y = _x;
        while (z < y) {
            y = z;
            z = (_x / z + z) / 2;
        }
    }
    
    function getSqrtPriceX96(uint priceA, uint priceB) public view returns (uint) {
        uint ratioX192 = (priceA << 192) / priceB;
        return _sqrt(ratioX192);
    }

